	public function Animations() {
		animationsMap = new dict();
		animationsMap.update("wife_generic_face", this.buildGenericWifeFaceAnimation());
		animationsMap.update("wife_left_arm", this.getLeftArmAnimation());
		animationsMap.update("wife_right_arm", this.getRightArmAnimation());
		animationsMap.update("husband_banker", this.buildBankerAnimation());
		animationsMap.update("husband_doctor", this.buildDoctorAnimation());
		animationsMap.update("husband_lawyer", this.buildLawyerAnimation());
		animationsMap.update("husband_exec", this.buildExecutiveAnimation());
		animationsMap.update("husband_sports", this.buildSportsAnimation());
		//animationsMap.update("gift_shop_balloons", this.getGiftShopBalloonsAnimation());
		animationsMap.update("gift_shop_people", this.getGiftShopIdleAnimation());
		animationsMap.update("gift_shop_end", this.getGiftShopAnimation());
		animationsMap.update("button_shine", this.getButtonShineAnimation());
		animationsMap.update("loading", this.getLoadingAnimation());
	}

	public function getAnimation(strAnimationName) {
	 	var action = animationsMap.get(strAnimationName);
		if (action != null) {
			action.stop();
			return action;
		}
		return null;
	}

