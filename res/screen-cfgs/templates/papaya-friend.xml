<?xml version="1.0" encoding="UTF-8" ?><screen:templates>	<screen:template name="PapayaFriend">		<screen:element name="friendContainer" resource="friend-belt/friendbelt-box.png"			left="#left_pos" top="0" z-index="10001" ontap="#tap_event">			<screen:element name="friendAvatar" resource="#avatar_url"				left="#avatar_left" top="#avatar_top"></screen:element>			<screen:element name="friendNameTxt" resource="" left="10"				top="140" text-wrap="yes" width="140" height="35" font-size="24"				font-style="normal" text-align="center">#friend_name</screen:element>			<screen:element name="inviteRibbon" left="0" top="0" resource="friend-belt/friendbelt-invite.png" visible="#invite"/>		</screen:element>	</screen:template></screen:templates>
