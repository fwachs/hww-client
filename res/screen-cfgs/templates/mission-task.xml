            <screen:element name="missionTaskText001" resource="" left="113" top="18" font-size="28" font-style="bold" width="600" height="36" text-align="left">Place #amount #furnitureName </screen:element>
            <screen:element name="missionTaskCheck" resource="mission/mission-check.png" left="0" top="-12" visible="#visible" />
