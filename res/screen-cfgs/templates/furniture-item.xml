<?xml version="1.0" encoding="UTF-8" ?><screen:templates>    <screen:template name="FurnitureItem">		<screen:element name="" resource="house-decorator/item-box.png" left="#left" top="15" width="183" height="216" ontap="furnitureTapped">			<screen:element name="" resource="#resource" left="15" top="26" width="163" height="163"/>			<screen:element name="" resource="" left="0" top="5" width="183" text-align="center" text-wrap="yes" font-size="24">#name</screen:element>						<screen:element name="" resource="house-decorator/star-faded.png" left="145" top="40"/>			<screen:element name="" resource="house-decorator/star-faded.png" left="145" top="75"/>			<screen:element name="" resource="house-decorator/star-faded.png" left="145" top="110"/>						<screen:element name="" resource="house-decorator/star-full.png" left="145" top="40" visible="#star_3_full"/>			<screen:element name="" resource="house-decorator/star-full.png" left="145" top="75" visible="#star_2_full"/>			<screen:element name="" resource="house-decorator/star-full.png" left="145" top="110" visible="#star_1_full"/>						<screen:element name="" resource="house-decorator/price-box.png" left="0" top="171" width="183" height="45">				<screen:element name="" resource="house-decorator/gamebucks.png" left="5" top="10"/>				<screen:element name="" resource="" left="35" top="10" font-size="24">#gamebucks</screen:element>						<screen:element name="" resource="house-decorator/diamond.png" left="115" top="10"/>				<screen:element name="" resource="" left="145" top="10" font-size="24">#diamonds</screen:element>			</screen:element>		</screen:element>	</screen:template>    <screen:template name="LockedFurnitureItem">		<screen:element name="" resource="house-decorator/locked-item-box-filter.png" left="#left" top="15" width="183" height="216" ontap="">			<screen:element name="" resource="#resource" left="15" top="26" width="163" height="163"/>			<screen:element name="" resource="house-decorator/price-box.png" left="0" top="171" width="183" height="45">				<screen:element name="" resource="house-decorator/lock-icon-small.png" left="5" top="-10"/>				<screen:element name="" resource="" left="55" top="10" font-size="24" font-style="bold">Level #level</screen:element>			</screen:element>		</screen:element>	</screen:template></screen:templates>